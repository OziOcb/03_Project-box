// ==================== Contact-Section ==================== 
section.contact#contact
  .contact__form
    h2.common__title contact<span>Us</span>
    
    form.form.home-wrap(action="https://example.net/comment.php" method="post")
      h3.visuallyhidden contact us form
      .form__wrapper
        p.form__input-wrap
          label.visuallyhidden(for="form__name") your name
          input.form__input(class="form__item" type="text" name="name" id="form__name" placeholder="your name" required)
        p.form__input-wrap
          label.visuallyhidden(for="form__email") email address
          input.form__input(class="form__item" type="email" name="email" id="form__email" placeholder="email address" required)
        p.form__input-wrap
          label.visuallyhidden(for="form__subject") subject
          input.form__input(class="form__item" type="text" name="subject" id="form__subject" placeholder="subject" required)
      p.form__input-wrap
        label.visuallyhidden(for="form__message") your message
        textarea.form__txt-area(class="form__item" name="message" id="form__message" placeholder="your message" required aria-required="true")
      p.form__input-wrap
        button.cta.form__btn(name="submit" type="submit" id="form-submit") submit now

